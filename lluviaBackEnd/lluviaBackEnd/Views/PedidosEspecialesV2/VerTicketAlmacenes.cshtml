
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.title</title>
    <style type="text/css">
        html, body, div, iframe {
            margin: 0;
            padding: 0;
            height: 100%;
        }

        iframe {
            display: block;
            width: 100%;
            border: none;
        }
    </style>
</head>


<body style="margin:0px;padding:0px;overflow:hidden">
    <div>
        <iframe src="data:application/pdf;base64,@ViewBag.pdfBase64" frameborder="0" style="overflow:hidden;height:100%;width:100%" height="100%" width="100%"></iframe>
    </div>
</body>
</html>
